{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "downlevelIteration": true,
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@groupdocs.examples.angular/common-components": [ 
        "dist/libs/common-components",     
        "libs/common-components/src/index.ts"
      ],
      "@groupdocs.examples.angular/viewer": ["libs/viewer/src/index.ts"],
      "@groupdocs.examples.angular/search": ["libs/search/src/index.ts"],
      "@groupdocs.examples.angular/editor": ["libs/editor/src/index.ts"],
      "@groupdocs.examples.angular/comparison": ["libs/comparison/src/index.ts"],
      "@groupdocs.examples.angular/signature": ["libs/signature/src/index.ts"],
      "@groupdocs.examples.angular/conversion": ["libs/conversion/src/index.ts"],
      "@groupdocs.examples.angular/annotation": ["libs/annotation/src/index.ts"],
      "@groupdocs.examples.angular/metadata": ["libs/metadata/src/index.ts"],
      "@groupdocs.examples.angular/parser": ["libs/parser/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
